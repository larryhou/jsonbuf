<class name="AssetStoreData">
    <field name="Groups" type="array">
        <array type="class">
            <class name="AssetGroup">
                <field name="Category" enum="EAssetCategory" type="int"/>
                <field name="Tag" enum="EAssetTag" type="int"/>
                <field name="Name" type="string"/>
                <field name="AssetInfo" type="array">
                    <array type="class">
                        <class name="AssetMetaData">
                            <field name="ID" type="int"/>
                            <field name="Path" type="string"/>
                            <field name="MidQualityPath" type="string"/>
                            <field name="LowQualityPath" type="string"/>
                            <field name="LoadMode" type="int"/>
                        </class>
                        <filter name="BuildToMobile" type="bool">true</filter>
                    </array>
                </field>
            </class>
        </array>
    </field>
</class>