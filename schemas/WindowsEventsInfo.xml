<class name="WwiseEventMeta">
    <field name="Infos" type="array">
        <array type="class">
            <class name="WwiseEventInfo">
                <field name="Id" type="uint"/>
                <field name="SqrtMaxAttenuation" type="float"/>
                <field name="DurationMax" type="float"/>
                <field name="RangeCheck" type="bool"/>
            </class>
        </array>
    </field>
    <field name="EventRedirectInfos" type="array">
        <array type="class">
            <class name="EventRedirectInfo">
                <field name="Name" type="string"/>
                <field name="SwitchGroup" type="string"/>
                <field name="SwitchValues" type="array">
                    <array type="string"/>
                </field>
                <field name="RedirectedEvents" type="array">
                    <array type="string"/>
                </field>
            </class>
        </array>
    </field>
</class>